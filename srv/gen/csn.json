{
  "namespace": "anubhav.db",
  "definitions": {
    "anubhav.db.CDSViews": {
      "kind": "context"
    },
    "anubhav.db.CDSViews.POWorklist": {
      "kind": "entity",
      "query": {
        "SELECT": {
          "from": {
            "ref": [
              "anubhav.db.transaction.purchaseorder"
            ]
          },
          "columns": [
            {
              "key": true,
              "ref": [
                "PO_ID"
              ],
              "as": "PurchaseOrderId"
            },
            {
              "ref": [
                "PARTNER_GUID",
                "BP_ID"
              ],
              "as": "PartnerId"
            },
            {
              "ref": [
                "PARTNER_GUID",
                "COMPANY_NAME"
              ],
              "as": "CompanyName"
            },
            {
              "ref": [
                "GROSS_AMOUNT"
              ],
              "as": "POGrossAmount"
            },
            {
              "ref": [
                "Currency",
                "code"
              ],
              "as": "POCurrencyCode"
            },
            {
              "ref": [
                "LIFECYCLE_STATUS"
              ],
              "as": "POStatus"
            },
            {
              "key": true,
              "ref": [
                "Items",
                "PO_ITEM_POS"
              ],
              "as": "ItemPosition"
            },
            {
              "ref": [
                "Items",
                "PRODUCT_GUID",
                "PRODUCT_ID"
              ],
              "as": "ProductId"
            },
            {
              "ref": [
                "Items",
                "PRODUCT_GUID",
                "DESCRIPTION"
              ],
              "as": "ProductName"
            },
            {
              "ref": [
                "PARTNER_GUID",
                "ADDRESS_GUID",
                "CITY"
              ],
              "as": "City"
            },
            {
              "ref": [
                "PARTNER_GUID",
                "ADDRESS_GUID",
                "COUNTRY"
              ],
              "as": "Country"
            },
            {
              "ref": [
                "Items",
                "GROSS_AMOUNT"
              ],
              "as": "GrossAmount"
            },
            {
              "ref": [
                "Items",
                "NET_AMOUNT"
              ],
              "as": "NetAmount"
            },
            {
              "ref": [
                "Items",
                "TAX_AMOUNT"
              ],
              "as": "TaxAmount"
            },
            {
              "ref": [
                "Items",
                "Currency",
                "code"
              ],
              "as": "CurrencyCode"
            }
          ]
        }
      },
      "elements": {
        "PurchaseOrderId": {
          "key": true,
          "type": "cds.String",
          "length": 24
        },
        "PartnerId": {
          "type": "cds.String",
          "length": 32
        },
        "CompanyName": {
          "type": "cds.String",
          "length": 250
        },
        "POGrossAmount": {
          "@Semantics.amount.currencyCode": "CURRENCY_CODE",
          "@sap.unit": "CURRENCY_CODE",
          "type": "anubhav.common.AmountT",
          "precision": 15,
          "scale": 2
        },
        "POCurrencyCode": {
          "@title": "{i18n>CurrencyCode}",
          "@Common.Text": {
            "=": "name"
          },
          "type": "cds.String",
          "length": 3
        },
        "POStatus": {
          "type": "cds.String",
          "length": 1
        },
        "ItemPosition": {
          "key": true,
          "type": "cds.Integer"
        },
        "ProductId": {
          "type": "cds.String",
          "length": 28
        },
        "ProductName": {
          "localized": true,
          "type": "cds.String",
          "length": 255
        },
        "City": {
          "type": "cds.String",
          "length": 44
        },
        "Country": {
          "type": "cds.String",
          "length": 44
        },
        "GrossAmount": {
          "@Semantics.amount.currencyCode": "CURRENCY_CODE",
          "@sap.unit": "CURRENCY_CODE",
          "type": "anubhav.common.AmountT",
          "precision": 15,
          "scale": 2
        },
        "NetAmount": {
          "@Semantics.amount.currencyCode": "CURRENCY_CODE",
          "@sap.unit": "CURRENCY_CODE",
          "type": "anubhav.common.AmountT",
          "precision": 15,
          "scale": 2
        },
        "TaxAmount": {
          "@Semantics.amount.currencyCode": "CURRENCY_CODE",
          "@sap.unit": "CURRENCY_CODE",
          "type": "anubhav.common.AmountT",
          "precision": 15,
          "scale": 2
        },
        "CurrencyCode": {
          "@title": "{i18n>CurrencyCode}",
          "@Common.Text": {
            "=": "name"
          },
          "type": "cds.String",
          "length": 3
        }
      }
    },
    "anubhav.db.CDSViews.ProductValueHelp": {
